{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/appauthentication",
      "DownstreamScheme": "https",
      "DangerousAcceptAnyServerCertificateValidator": "true",
      "UpstreamPathTemplate": "/appauthentication",
      "ServiceName": "luis-middleware-identity",
      "UpstreamHttpMethod": [ "Post" ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    },
    {
      "DownstreamPathTemplate": "/api/luisdiscovery",
      "DownstreamScheme": "https",
      "DangerousAcceptAnyServerCertificateValidator": "true",
      "UpstreamPathTemplate": "/luisdiscovery",
      "ServiceName": "luis-middleware",
      "UpstreamHttpMethod": [ "Post" ],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://luis-middleware-gateway:433",
    "ServiceDiscoveryProvider": {
      "Host": "consul",
      "Port": 8500,
      "Type": "Consul"
    }
  }
}